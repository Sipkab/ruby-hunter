		<enum name="Terminated"			value="13"/>
		<enum name="NewerVersion"		value="14"/>
		<enum name="SameHardware"		value="15"/>
		<enum name="AlreadyLinked"		value="16"/>
		<enum name="NoStatsYet"			value="17"/>
